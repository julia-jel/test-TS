  "scripts": {
    "lint": "eslint --ext=.js,.jsx,.ts,.tsx --cache=true src/ webpack/ *.js",
    "lint:prod": "eslint --config .eslintrc-prod.yaml --ext .js,.jsx,.ts,.tsx src/ webpack/*.js",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix --cache=true src/ webpack/ *.js",
    "spellcheck": "cspell --config ./cspell.json $(find ./src -name \\*.js)",
    "clean": "rimraf ./build/**",
    "_build": "npm run clean && webpack --config ./webpack/config.babel.js --progress",
    "build:prod": "cross-env NODE_ENV='production' npm run _build",
    "build:dev": "cross-env NODE_ENV='development' npm run _build",
    "build": "npm run build:prod",
    "start:hot": "npm run clean && cross-env HOT=true NODE_ENV='development' babel-node ./server-dev.js",
    "dev": "npm run start:hot",
    "main:prod": "cross-env NODE_ENV='production' babel-node ./server-prod.js",
    "start": "npm run start:prod",
    "check-types": "tsc",
    "check": "npm run lint:prod && npm run check-types && npm run spellcheck && npm run build"
  },
